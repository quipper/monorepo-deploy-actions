  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.14.2",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-plugin-jest": "27.2.1",
    "prettier": "2.8.6",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
